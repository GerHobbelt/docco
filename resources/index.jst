<!DOCTYPE html>

<html>
<head>
  <title><%= title %></title>
  <meta http-equiv="content-type" content="text/html; charset=UTF-8">
  <link rel="stylesheet" media="all" href="stylesheets/index.css" />
</head>
<body>
  <div id="container">
   <div id="background"></div>
   <table cellpadding="0" cellspacing="0">
   <tbody>
   <tr>
   <td>
    <div style="position:absolute;top:0px;left:0px;width:525px">
    <a href="https://github.com/<%= user %>/<%= repo %>"><img src="https://s3.amazonaws.com/github/ribbons/forkme_right_red_aa0000.png" alt="Fork me on GitHub"></a>
    </div>
    <h1><%= title %></h1>
    <p><%= description %></p>
    <!--readme-->
    </td>
    <td></td>
    </tr>
    <tr>
    <td>
		<div class="readme"><%= readme %></div>
    </td>
    <td>
    <!--content-->
		<div class="content">

      <table class="code_stats">
        <% statistics.forEach(function(entry) { %>
        <tr>
          <td><%= entry[0] %></td>
          <td><%= entry[2] %></td>
          <td><span data-lang="<%= entry[2] %>"></span></td>
	  <td style="width:400px;"><div><span data-lang="<%= entry[2] %>" percent="<%= entry[0] %>" style="width: 0%;"></span><span><%= entry[1] %> sloc</span></div></td>
        </tr>
        <tr>
        <% }); %>
      </table>
     
      <ol>
        <% for (var i = 0, l = log.length; i < l; i++) { %>
        <% var entry = log[i]; %>
        <li><%= entry.date %>, <%= entry.author %>, <%= entry.subject %></li>
        <% } %>
      </ol>

  
    <div id="filebrowser">
      <div id="breadcrumb"></div>
      <div id="filelists"></div>
    </div>
  
    </div>
   </td>
   </tr>
   </tbody>
   </table>
 </div>
<script>
document.write('<script src=' +
('__proto__' in {} ? 'http://cdnjs.cloudflare.com/ajax/libs/zepto/1.0rc1/zepto.min.js':'https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js') +
'><\\/script>')
</script>
<script src="http://documentcloud.github.com/underscore/underscore-min.js"></script>
<script src="http://baoshan.github.com/moment/min/moment.min.js"></script>
<script src="javascript/index.js"></script>
<script>$(function(){new file_browser("<%= user %>","<%= repo %>","docas/tree/docas.index")})</script>
</body>
</html>
